{
  "name": "@candlefw/whind",
  "version": "0.2.1",
  "description": "A Simple Tokenizer / Lexer",
  "main": "./build/whind.mjs",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test.watch": "mocha ./test/test.js --watch",
    "test": "nyc mocha ./test/test.js",
    "build": "rollup --config ./build/config/config.js",
    "build.watch": "rollup --config ./build/config/config.js --watch",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/galactrax/cfw_whind"
  },
  "keywords": [
    "lexer",
    "string",
    "parser",
    "parse",
    "tokenizer"
  ],
  "author": "Anthony Weathersby",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rollup": "^0.67.3"
  },
  "jshintConfig": {
    "esversion": 6,
    "unused": true
  }
}
